 "boxes" : [ { "box" : 				{
					"code" : " require(\"migen-lib\");\r\nrequire(\"migen-integrated\");\r\nrequire(\"migen-proxies\");\r\nBuffer motion;\r\n\r\n// Model data structures\r\nData proxy_18(3);\r\nData proxy_17(3);\r\nData proxy_16(3);\r\nData proxy_15(3);\r\nData proxy_14(3);\r\nData proxy_13(3);\r\nData proxy_12(3);\r\nData proxy_11(3);\r\nData proxy_10(3);\r\nData proxy_9(3);\r\nData proxy_8(3);\r\nData proxy_7(3);\r\nData proxy_6(3);\r\nData proxy_5(3);\r\nData proxy_4(3);\r\nData proxy_3(3);\r\nData proxy_2(3);\r\nData proxy_1(3);\r\nData proxy_0(3);\r\nData m_in5(3);\r\nData m_in3(3);\r\nData m_in1(3);\r\nData m2(3);\r\nData m(3);\r\nData g2(3);\r\nData g(3);\r\nData s(3, 120);\r\n\r\n// Control Rate Parameters\r\nParam nlK(0.2);\r\nParam fZ(0.05);\r\nParam fK(0.7);\r\n\r\nParam display_motion(0);\r\n\r\n// Model initialisation flag\r\nHistory model_init(0);\r\nHistory render_cpt(0);\r\n\r\n// Audio Rate Parameters\r\nfinger2 = in4;\r\nfinger1 = in2;\r\n\r\n// Model init phase\r\n\r\nif(model_init == 0){\r\ninit_mat(proxy_18, 0, 0);\r\ninit_mat(proxy_17, 0, 0);\r\ninit_mat(proxy_16, 0, 0);\r\ninit_mat(proxy_15, 0, 0);\r\ninit_mat(proxy_14, 0, 0);\r\ninit_mat(proxy_13, 0, 0);\r\ninit_mat(proxy_12, 0, 0);\r\ninit_mat(proxy_11, 0, 0);\r\ninit_mat(proxy_10, 0, 0);\r\ninit_mat(proxy_9, 0, 0);\r\ninit_mat(proxy_8, 0, 0);\r\ninit_mat(proxy_7, 0, 0);\r\ninit_mat(proxy_6, 0, 0);\r\ninit_mat(proxy_5, 0, 0);\r\ninit_mat(proxy_4, 0, 0);\r\ninit_mat(proxy_3, 0, 0);\r\ninit_mat(proxy_2, 0, 0);\r\ninit_mat(proxy_1, 0, 0);\r\ninit_mat(proxy_0, 0, 0);\r\ninit_mat(m_in5, 0., 0.);\r\ninit_mat(m_in3, 0., 0.);\r\ninit_mat(m_in1, 0., 0.);\r\ninit_mat(m2, 0., 0.);\r\ninit_mat(m, 0., 0.);\r\ninit_mat(g2, -0.2, -0.2);\r\ninit_mat(g, -0.1, -0.1);\r\ninit_multiple_masses(s, 0., 0.);\r\n\r\n// Raise init flag\r\nmodel_init = 1;\r\n}\r\n\r\n// Model computation\r\nupdate_input_pos(m_in5, in5);\r\nupdate_input_pos(m_in3, in3);\r\nupdate_input_pos(m_in1, in1);\r\ncompute_mass(m2, 100000);\r\ncompute_mass(m, 